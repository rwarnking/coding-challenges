{
    "extends": ["eslint:recommended"],
    "env": { "es6": true },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "script",
        "ecmaFeatures": {}
    },
    "ignorePatterns": [
    ],
    "rules": {
        "indent": ["error", 4, { "outerIIFEBody": "off", "SwitchCase": 1 }],
        "no-undef": "off",
        "no-unused-vars": "off",
        "max-len": ["error", {
            "code": 100,
            "ignoreComments": false,
            "ignoreStrings": false,
            "ignoreTemplateLiterals": false,
            "ignoreRegExpLiterals": false
        }]
    }
}
